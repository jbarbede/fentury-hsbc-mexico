{
  "name": "ZonDuplicatedOrders",
  "short_name": "ZonDuplicatedOrders",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Find easily duplicated Amazon orders.",
  "homepage_url": "https://zonpages.com",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/19.png",
    "default_title": "ZonDuplicatedOrders",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["assets/background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://connect.facebook.net; object-src 'self'",
  "permissions": [
    "alarms", "notifications", "tabs",
    "*://sellercentral.amazon.com/orders-api*",
    "*://sellercentral.amazon.com/hz/orders*"
  ]
}