{
  "name": "fentury.hsbc.mexico",
  "version": "0.0.1",
  "description": "The Fentury HSBC Mexico Chrome extension using ES6, Bootstrap 4 and webpack",
  "main": "index.js",
  "scripts": {
    "watch": "rm -rf ../extension/assets/ && webpack --mode development --config webpack.dev.js --progress --colors --watch",
    "build": "rm -rf ../extension/assets/ && webpack --mode development --config webpack.dev.js --progress --colors",
    "buildp": "rm -rf ../extension/assets/ && webpack --mode production --config webpack.prod.js --progress --colors",
    "stats": "webpack --mode production --config webpack.prod.js --profile --json > webpack-stats.json"
  },
  "author": "Fentury team <team@fentury.com> (https://fentury.com)",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.8.2",
    "@fortawesome/free-brands-svg-icons": "5.8.2",
    "@fortawesome/free-regular-svg-icons": "5.8.2",
    "bootstrap": "4.3.1",
    "jquery": "3.4.1",
    "md5": "2.2.1",
    "moment": "2.24.0",
    "popper.js": "1.15.0",
    "pouchdb": "7.0.0"
  },
  "homepage": "https://fentury.com",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-loader": "8.0.6",
    "css-loader": "2.1.1",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "node-sass": "4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "path": "0.12.7",
    "postcss-loader": "3.0.0",
    "requirejs": "2.3.6",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "ts-loader": "6.0.2",
    "typescript": "3.5.1",
    "webpack": "4.32.2",
    "webpack-clean-obsolete-chunks": "0.4.0",
    "webpack-cli": "3.3.2",
    "webpack-merge": "4.2.1"
  }
}
