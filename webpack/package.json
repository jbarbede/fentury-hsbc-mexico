{
  "name": "zon.orders.duplicated",
  "version": "0.0.1",
  "description": "The ZonDuplicatedOrders Chrome extension using ES6, Bootstrap 4 and webpack",
  "main": "index.js",
  "scripts": {
    "watch": "rm -rf ../extension/assets/ && webpack --mode development --config webpack.dev.js --progress --colors --watch",
    "build": "rm -rf ../extension/assets/ && webpack --mode development --config webpack.dev.js --progress --colors",
    "buildp": "rm -rf ../extension/assets/ && webpack --mode production --config webpack.prod.js --progress --colors",
    "stats": "webpack --mode production --config webpack.prod.js --profile --json > webpack-stats.json"
  },
  "author": "Zonpages.com team <team@zonpages.com> (https://zonpages.com)",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.2.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "npm": "^6.3.0",
    "popper.js": "^1.14.4",
    "pouchdb": "^7.0.0"
  },
  "homepage": "https://zonpages.com",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "node-sass": "^4.9.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "requirejs": "^2.3.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}
